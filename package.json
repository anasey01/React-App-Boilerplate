{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "description": "A React Application boilerplate, webpack, babel ",
  "main": "index.js",
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "express": "^4.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "1.17.0",
    "react-hot-loader": "^4.8.3",
    "style-loader": "^0.23.1"
  },
  "scripts": {
    "start": "webpack && npm run build && node server/dist/app.js",
    "build": "rm -rf server/dist && babel server --out-dir server/dist",
    "development": "webpack-dev-server --config ./webpack.config.js --mode development --open --hot --inline",
    "dev": "npm run development",
    "prod": "npm run production",
    "production": "npm run start",
    "format": "prettier --write \"src/**/*.jsx\"",
    "precommit": "lint-staged && npm test",
    "test": "jest"
  },
  "lint-staged": {
    ".jsx": [
      "npm run format",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anasey01/React-App-Boilerplate.git"
  },
  "author": "Oluseyi Anani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anasey01/React-App-Boilerplate/issues"
  },
  "homepage": "https://github.com/anasey01/React-App-Boilerplate#readme"
}
