{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "description": "A React Application boilerplate, webpack, babel ",
  "main": "index.js",
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "clean-webpack-plugin": "^2.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "react-hot-loader": "^4.8.3",
    "redux-devtools": "^3.5.0",
    "style-loader": "^0.23.1"
  },
  "scripts": {
    "start": "npm run build && node server/dist/app.js",
    "build-server": "rm -rf server/dist/ && babel server --out-dir server/dist",
    "build": "webpack --config ./webpack.prod.js && npm run build-server",
    "development": "webpack-dev-server --config ./webpack.dev.js --open --hot --inline",
    "dev": "npm run development",
    "prod": "npm run production",
    "production": "npm run start",
    "format": "prettier --write \"src/**/*.jsx\"",
    "precommit": "lint-staged && npm test",
    "test": "jest --config ./jest.config.json --coverage --colors"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anasey01/React-App-Boilerplate.git"
  },
  "author": "Oluseyi Anani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anasey01/React-App-Boilerplate/issues"
  },
  "homepage": "https://github.com/anasey01/React-App-Boilerplate#readme"
}
